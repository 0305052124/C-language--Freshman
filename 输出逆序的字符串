//// 输出逆序的字符串
#include<stdio.h> 
int main()
{
	char arr[] = "fwiuzv";
	int sz = sizeof(arr) / sizeof(arr[0]);
	int b[10000];
	for (int i = 0; i < sz; i++)
	{
		b[i] = arr[sz - 1 - i];
	}
	for (int i = 0; i < sz; i++)
	{
		 printf("%c",b[i]);
	}
	printf("\n");
	char c[] = "nowcoder";
	int size = sizeof(c) / sizeof(c[0]); 
	int ret;
	for (int i = 0; i < (size/2); i++)
	{
		ret= c[i];
		c[i] = c[size - 1 - i];
		c[size - 1 - i] = ret;
	}
	for (int i = 0; i < size; i++)
	{
		printf("%c", c[i]);
	}
	return 0;
}
